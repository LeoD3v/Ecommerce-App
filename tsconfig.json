{
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "types": ["node", "jest"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx",]
}
